{
    "env": { 
        "jquery": true, 
        "browser": true,
        "es6": true
    },
    "plugins": [
        "html"
      ],
    "extends": "eslint:recommended",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
    "indent": ["warn", 4, { "SwitchCase": 1 }],
    "linebreak-style": ["error", "windows"],
    "quotes": ["error", "double"],
    "semi": ["error", "always"],
    "no-console": 0

    }
}